		when t_task_41 < C_task_41 & t_pipeline_4 = D_pipeline_4 & p_global_time = global_time sync DEADLINE_MISSED_3 goto DEADLINE_MISS;
		when t_task_23 = C_task_23 sync task_23_done goto proc_3_001;
		when t_task_23 < C_task_23 sync task_31_act do {t_task_31'=0} goto proc_3_111;
		when t_task_41 = C_task_41 sync task_41_done goto proc_3_100;
	loc proc_3_011: while t_task_31 <= C_task_31 stop {t_task_23,t_task_41} wait
		when t_task_41 < C_task_41 & t_pipeline_1 = D_pipeline_1 & p_global_time = global_time sync DEADLINE_MISSED_3 goto DEADLINE_MISS;
		when t_task_41 < C_task_41 & t_pipeline_2 = D_pipeline_2 & p_global_time = global_time sync DEADLINE_MISSED_3 goto DEADLINE_MISS;
